{
  "engines": {
    "node": ">=12.21.0",
    "npm": ">=6.14.11"
  },
  "name": "overhide-ethereum",
  "version": "1.0.0",
  "description": "Ethereum Remuneration API for overhide",
  "main": "main/js/overhide-ethereum.js",
  "keywords": [
    "overhide",
    "ethereum",
    "remuneration",
    "api"
  ],
  "author": "Jakub Ner",
  "license": "MIT",
  "dependencies": {
    "@godaddy/terminus": "4.1.0",
    "@keyv/redis": "1.3.8",
    "applicationinsights": "2.1.4",
    "cors": "2.8.5",
    "debug": "4.1.0",
    "ejs": "2.6.1",
    "express": "4.16.4",
    "express-rate-limit": "3.3.2",
    "js-yaml": "^3.13.1",
    "keyv": "3.1.0",
    "license-compatibility-checker": "^0.3.4",
    "node-fetch": "3.1.1",
    "pg": "8.5.1",
    "rate-limit-redis": "2.1.0",
    "read": "1.0.7",
    "run-script-os": "1.0.3",
    "swagger-jsdoc": "^3.2.9",
    "uuid": "3.3.2",
    "web3": "1.3.4"
  },
  "config": {
    "OH_ETH_HOST": "localhost",
    "OH_ETH_PORT": 8080,
    "BASE_URL": "localhost:8080",
    "DEBUG": "",
    "TOKEN_URL": "https://token.overhide.io/validate",
    "INTERNAL_TOKEN": "",
    "SALT": "",
    "ISPROD": "",
    "ETHERSCAN_KEY": "",
    "WEB3_URI": "",
    "NETWORK_TYPE": "",
    "EXPECTED_CONFIRMATIONS": 7,
    "RATE_LIMIT_FE_WINDOW_MS": 1000,
    "RATE_LIMIT_FE_MAX_REQUESTS_PER_WINDOW": 100,
    "RATE_LIMIT_FE_REDIS_URI": "",
    "RATE_LIMIT_FE_REDIS_NAMESPACE": "",
    "RATE_LIMIT_BE_WINDOW_MS": 1000,
    "RATE_LIMIT_BE_MAX_REQUESTS_PER_WINDOW": 100,
    "RATE_LIMIT_BE_REDIS_URI": "",
    "RATE_LIMIT_BE_REDIS_NAMESPACE": "",
    "KEYV_TALLY_CACHE_URI": "",
    "POSTGRES_HOST": "localhost",
    "POSTGRES_PORT": 5432,
    "POSTGRES_DATABASE": "",
    "POSTGRES_USER": "",
    "POSTGRES_PASSWORD": "",
    "POSTGRES_SSL": ""
  },
  "devDependencies": {
    "mocha": "5.2.0",
    "mocha-junit-reporter": "1.22.0",
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "nodemon": "2.0.7"
  },
  "scripts": {
    "lic-check": "license-compatibility-checker",
    "start": "node main/js/overhide-ethereum.js",
    "test": "mocha --exit --timeout 5000 test/js",
    "test-report": "mocha --exit --timeout 5000 test/js --reporter mocha-junit-reporter --reporter-options toConsole=true,mochaFile=test-report/test-report.xml",
    "dev": "nodemon --inspect main/js/overhide-ethereum.js",
    "build": "docker build -t oh-eth -f main/docker/Dockerfile .",
    "compose-dev": "docker-compose -f main/docker/docker-compose.yml -f main/docker/docker-compose.dev.yml --project-directory . --project-name oh up -d",
    "clean": "run-script-os",
    "clean:win32": "@powershell docker rm (docker kill (docker ps -a -q --filter name=oh-eth)) ; docker rmi oh-eth:latest",
    "clean:darwin:freebsd:linux:sunos": "docker rm $(docker kill $(docker ps -a -q --filter name=oh-eth)) ; docker rmi oh-eth:latest",
    "psql-dev": "docker run -it --rm --link postgres:postgres -e PGPASSWORD=c0c0nut --network oh_default postgres psql -h postgres -U adam -d postgres",
    "redis-cli": "docker exec -it redis redis-cli",
    "db-evolve": "node tools/db-evolve/db-evolve.js"
  }
}
